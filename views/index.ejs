<!DOCTYPE html>
<html lang="en">
    <head>
        <!-- Required meta tags -->
        <meta charset="utf-8">
            <meta content="width=device-width, initial-scale=1, shrink-to-fit=no" name="viewport"/>
            <title>
                Trade Coin Tool
            </title>
            <!-- plugins:css -->
            <link href="../dest/css/style.min.css" rel="stylesheet">
                <!-- endinject -->
                <link href="../images/favicon.png" rel="shortcut icon"/>
            </link>
        </meta>
    </head>
    <body>
        <div class="container-scroller">
            <!-- partial:partials/_navbar.html -->
            <% include ../views/partials/navbar %>
            <!-- partial -->
            <div class="container-fluid page-body-wrapper">
                <!-- partial:partials/_sidebar.html -->
                <% include ../views/partials/sidebar %>
                <!-- partial -->
                <div class="main-panel">
                    <div class="content-wrapper">
                        <div class="row">
                            <div class="col-12 grid-margin">
                                <div class="card">
                                    <div class="card-body">
                                        <div class="form-group row">
                                            <h5 class="col-9 card-title">
                                                Coin đã mua
                                            </h5>
                                        </div>
                                        <div class="fluid-container">
                                            <div class="row" style="zoom: 0.85;">
                                                <table class="table table-hover" id="tickets" style="width:100%;">
                                                    <thead class="thead-light">
                                                        <tr>
                                                            <th>
                                                                STT
                                                            </th>
                                                            <th>
                                                                Thị trường
                                                            </th>
                                                            <th>
                                                                Giá mua
                                                            </th>
                                                            <th>
                                                                Giá hiện tại
                                                            </th>
                                                            <th>
                                                                Volume Rate
                                                            </th>
                                                            <th>
                                                                Chỉ số 1h
                                                            </th>
                                                            <th>
                                                                Chỉ số 5m
                                                            </th>
                                                            <th>
                                                                Hành động
                                                            </th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <% for(i in marketBuy) { %>
                                                    <% var market = marketBuy[i]['MarketName']; %> 
                                                    <% var self = marketBuy[i]; %> 
                                                    <% var i_1h = self.indicator_1h; %>
                                                    <% var i_5m = self.indicator_5m; %>
                                                    <% var stopmuaMaket = self.stopmua; %>
                                                    <% var pattern_1h = i_1h.pattern.pattern; %>
                                                    <% var probability_1h = Math.round(i_1h.pattern.probability); %>
                                                    <% var pattern_5m = i_5m.pattern.pattern; %>
                                                    <% var probability_5m = Math.round(i_5m.pattern.probability); %>
                                                    <% var count_buy_1h = i_1h.count_buy; %>
                                                    <% var count_sell_1h = i_1h.count_sell; %>
                                                    <% var rate_1h = i_1h.rate; %>
                                                    <% var count_buy_5m = i_5m.count_buy; %>
                                                    <% var count_sell_5m = i_5m.count_sell; %>
                                                    <% var rate_5m = i_5m.rate; %>
                                                    <% var price_buy = self.priceBuyAvg; %>
                                                    <% var bids_q = 0; %>
                                                    <% var asks_q = 0; %>
                                                    <% var rate = 0; %>
                                                    <% var profit = self.last - price_buy; %>
                                                    <% var percent = profit / price_buy * 100; %>
                                                    <% var classess = ''; %>
                                                    <% if(percent > 0){ %> 
                                                    <% classess = 'text-success'; %> 
                                                    <% }else{ %> 
                                                    <% classess = 'text-danger'; %> 
                                                    <% } %>
                                                        <tr data-symbol="<%= market %>">
                                                            <td>
                                                                <%= i++ %>
                                                            </td>
                                                            <td>
                                                                <a class="fancybox" data-symbol="<%= market %>" data-target="#myModal" data-toggle="modal" href="#">
                                                                    <%= market %>
                                                                </a>
                                                            </td>
                                                            <td>
                                                                <span class="price_buy" data-symbol="<%= market %>">
                                                                    <%= round(price_buy,8) %>
                                                                </span>
                                                            </td>
                                                            <td>
                                                                <span class="mr-2 price_last" data-symbol="<%= market %>">
                                                                    <%= markets[market].last %>
                                                                </span>
                                                                <span class="profit <%= percent > 0 ? 'text-success' : 'text-danger' %>" data-symbol="<%= market %>">
                                                                    (<%= round(percent,2) %>%)
                                                                </span>
                                                            </td>
                                                            <td class="text-center">
                                                                <b class="mr-2 text-success bids_q" data-symbol="<%= market %>">
                                                                    <%= round(bids_q,5) %>
                                                                </b>
                                                                <b class="text-danger asks_q" data-symbol="<%= market %>">
                                                                    <%= round(asks_q,5) %>
                                                                </b>
                                                                <p class="d-block">
                                                                    <span class='rate_order badge <%= bids_q > asks_q ? "badge-success" : "badge-danger" %>' data-symbol="<%= market %>">
                                                                        <%= round(rate,2) %>%
                                                                    </span>
                                                                </p>
                                                            </td>
                                                            <td class="text-center">
                                                                <b class="text-danger count_sell" data-interval="1h" data-symbol="<%= market %>">
                                                                    <%= count_sell_1h %>
                                                                </b>
                                                                <b class="text-success count_buy" data-interval="1h" data-symbol="<%= market %>">
                                                                    <%= count_buy_1h %>
                                                                </b>
                                                                <b class="d-block text-primary pattern" data-interval="1h" data-symbol="<%= market %>">
                                                                    <%= pattern_1h %>(<%= probability_1h %>)
                                                                </b>
                                                            </td>
                                                            <td class="text-center">
                                                                <b class="text-danger count_sell" data-interval="5m" data-symbol="<%= market %>">
                                                                    <%= count_sell_5m %>
                                                                </b>
                                                                <b class="text-success count_buy" data-interval="5m" data-symbol="<%= market %>">
                                                                    <%= count_buy_5m %>
                                                                </b>
                                                                <b class="d-block text-primary pattern" data-interval="5m" data-symbol="<%= market %>">
                                                                    <%= pattern_5m %>(<%= probability_5m %>)
                                                                </b>
                                                            </td>
                                                            <td>
                                                                <span class="p-2 setting icon-settings alert alert-primary" data-dismiss="modal" data-symbol="<%= market %>" data-target="#settingModal" data-toggle="modal">
                                                                </span>
                                                                <span class='badge stopmuaMaket <%= stopmuaMaket ? "badge-danger" : "badge-success" %>' data-symbol="<%= market %>">
                                                                    Auto buy
                                                                </span>
                                                            </td>
                                                        </tr>
                                                        <% } %>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="card-footer">
                                        <a data-target="#historyModel" data-toggle="modal" href="#">
                                            Lịch sử
                                        </a>
                                    </div>
                                </div>
                                <div class="card">
                                    <div class="card-body">
                                        <div class="form-group row">
                                            <h5 class="col-9 card-title" id="showhot">
                                                HOT Market
                                            </h5>
                                            <div class="col-3">
                                                <input class="form-control search" placeholder="Search" type="email">
                                                </input>
                                            </div>
                                        </div>
                                        <div class="fluid-container" id="volumn">
                                            <div class="row">
                                                <% for(market in marketHot) { %>
                                                <div class="col-6 ticket-card p-2" data-symbol="<%= market %>" id="market<%= market %>">
                                                    <div>
                                                        <%= market %>:
                                                        <span class="mr-2 price_last" data-symbol="<%= market %>">
                                                        </span>
                                                    </div>
                                                </div>
                                                <% } %>
                                            </div>
                                        </div>
                                        <div class="fluid-container" id="allmarket">
                                            <div class="row">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="card">
                                    <div class="card-body">
                                        <div class="col-6">
                                            <h5 class="col-12 card-title text-center" id="marketdynamic">
                                                Market Change
                                            </h5>
                                            <div id="scatterChart">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="card">
                                    <div class="card-body">
                                        <div class="form-group row">
                                            <h5 class="col-8 card-title" id="refreshcheck">
                                                All market
                                            </h5>
                                            <div class="col-4 small">
                                                Sắp xếp theo:
                                                <a class="order_by_trade" data-interval="1h" href="#">
                                                    Lượt giao dịch 1h
                                                </a>
                                                |
                                                <a class="order_by_trade" data-interval="5m" href="#">
                                                    Lượt giao dịch 5m
                                                </a>
                                                |
                                                <a class="order_by_trade" data-interval="1m" href="#">
                                                    Lượt giao dịch 1m
                                                </a>
                                                |
                                                <a class="order_by_change" href="#">
                                                    % thay đổi
                                                </a>
                                            </div>
                                        </div>
                                        <div class="fluid-container">
                                            <div class="row" style="zoom: 0.85;">
                                                <table class="table table-hover" id="BTC-market" style="width:100%;">
                                                    <thead class="thead-light">
                                                        <tr>
                                                            <th>
                                                                STT
                                                            </th>
                                                            <th>
                                                                Thị trường
                                                            </th>
                                                            <th>
                                                                Giá check
                                                            </th>
                                                            <th>
                                                                Giá hiện tại
                                                            </th>
                                                            <th>
                                                                Volume Rate
                                                            </th>
                                                            <th>
                                                                Chỉ số 1h
                                                            </th>
                                                            <th>
                                                                Chỉ số 5m
                                                            </th>
                                                            <th>
                                                                Hành động
                                                            </th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <% var i =1; %>
                                                <% for(market in markets) { %>
                                                <% var self = markets[market]; %> 
                                                <% var i_1h = self.indicator_1h; %>
                                                <% var i_5m = self.indicator_5m; %>
                                                <% var stopmuaMaket = self.stopmua; %>
                                                <% var pattern_1h = i_1h.pattern.pattern; %>
                                                <% var probability_1h = Math.round(i_1h.pattern.probability); %>
                                                <% var pattern_5m = i_5m.pattern.pattern; %>
                                                <% var probability_5m = Math.round(i_5m.pattern.probability); %>
                                                <% var count_buy_1h = i_1h.count_buy; %>
                                                <% var count_sell_1h = i_1h.count_sell; %>
                                                <% var rate_1h = i_1h.rate; %>
                                                <% var count_buy_5m = i_5m.count_buy; %>
                                                <% var count_sell_5m = i_5m.count_sell; %>
                                                <% var rate_5m = i_5m.rate; %>
                                                <% var price_check = self.price_check; %>
                                                <% var bids_q = 0; %>
                                                <% var asks_q = 0; %>
                                                <% var rate = 0; %>
                                                <% var profit = self.last - price_check; %>
                                                <% var percent = profit / price_check * 100; %>
                                                <% var classess = ''; %>
                                                <% if(percent > 0){ %> 
                                                <% classess = 'text-success'; %> 
                                                <% }else{ %> 
                                                <% classess = 'text-danger'; %> 
                                                <% } %>
                                                        <tr class="tr_market" data-order="<%= round(percent,2) %>" data-symbol="<%= market %>">
                                                            <td>
                                                                <%= i++ %>
                                                            </td>
                                                            <td>
                                                                <a class="fancybox" data-symbol="<%= market %>" data-target="#myModal" data-toggle="modal" href="#">
                                                                    <%= market %>
                                                                </a>
                                                            </td>
                                                            <td>
                                                                <span class="price_check" data-symbol="<%= market %>">
                                                                    <%= round(price_check,8) %>
                                                                </span>
                                                            </td>
                                                            <td>
                                                                <span class="mr-2 price_last" data-symbol="<%= market %>">
                                                                    <%= markets[market].last %>
                                                                </span>
                                                                <span class="profit_check <%= percent > 0 ? 'text-success' : 'text-danger' %>" data-symbol="<%= market %>">
                                                                    (<%= round(percent,2) %>%)
                                                                </span>
                                                            </td>
                                                            <td class="text-center">
                                                                <b class="mr-2 text-success bids_q" data-symbol="<%= market %>">
                                                                    <%= round(bids_q,5) %>
                                                                </b>
                                                                <b class="text-danger asks_q" data-symbol="<%= market %>">
                                                                    <%= round(asks_q,5) %>
                                                                </b>
                                                                <p class="d-block">
                                                                    <span class='rate_order badge <%= bids_q > asks_q ? "badge-success" : "badge-danger" %>' data-symbol="<%= market %>">
                                                                        <%= round(rate,2) %>%
                                                                    </span>
                                                                </p>
                                                            </td>
                                                            <td class="text-center">
                                                                <b class="text-danger count_sell" data-interval="1h" data-symbol="<%= market %>">
                                                                    <%= count_sell_1h %>
                                                                </b>
                                                                <b class="text-success count_buy" data-interval="1h" data-symbol="<%= market %>">
                                                                    <%= count_buy_1h %>
                                                                </b>
                                                                <b class="d-block text-primary pattern" data-interval="1h" data-symbol="<%= market %>">
                                                                    <%= pattern_1h %>(<%= probability_1h %>)
                                                                </b>
                                                            </td>
                                                            <td class="text-center">
                                                                <b class="text-danger count_sell" data-interval="5m" data-symbol="<%= market %>">
                                                                    <%= count_sell_5m %>
                                                                </b>
                                                                <b class="text-success count_buy" data-interval="5m" data-symbol="<%= market %>">
                                                                    <%= count_buy_5m %>
                                                                </b>
                                                                <b class="d-block text-primary pattern" data-interval="5m" data-symbol="<%= market %>">
                                                                    <%= pattern_5m %>(<%= probability_5m %>)
                                                                </b>
                                                            </td>
                                                            <td>
                                                                <span class="p-2 setting icon-settings alert alert-primary" data-dismiss="modal" data-symbol="<%= market %>" data-target="#settingModal" data-toggle="modal">
                                                                </span>
                                                                <span class='badge stopmuaMaket <%= stopmuaMaket ? "badge-danger" : "badge-success" %>' data-symbol="<%= market %>">
                                                                    Auto buy
                                                                </span>
                                                            </td>
                                                        </tr>
                                                        <% } %>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- content-wrapper ends -->
                    <!-- partial:partials/_footer.html -->
                    <% include ../views/partials/footer %>
                    <!-- partial -->
                </div>
                <!-- main-panel ends -->
            </div>
            <!-- page-body-wrapper ends -->
        </div>
        <!-- container-scroller -->
        <div aria-hidden="true" aria-labelledby="smallModalLabel" class="modal fade" id="myModal">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <!-- Modal body -->
                    <div class="modal-body">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-6">
                                    <div class="col-12">
                                        <div class="btn-group btn-group-toggle" data-toggle="buttons" id="interval">
                                            <label class="btn btn-secondary">
                                                <input autocomplete="off" name="options" type="radio" value="5m">
                                                    5m
                                                </input>
                                            </label>
                                            <label class="btn btn-secondary active">
                                                <input autocomplete="off" checked="" name="options" type="radio" value="1h">
                                                    1h
                                                </input>
                                            </label>
                                            <label class="btn btn-secondary">
                                                <input autocomplete="off" name="options" type="radio" value="1d">
                                                    1d
                                                </input>
                                            </label>
                                        </div>
                                        <button class="btn" id="tradingview-buton" type="button">
                                            Tradingview
                                        </button>
                                        <button class="btn setting icon-settings" data-dismiss="modal" data-symbol="" data-target="#settingModal" data-toggle="modal" href="#" type="button">
                                        </button>
                                        <button class="btn hotmarket icon-star" data-symbol="" type="button">
                                        </button>
                                    </div>
                                    <div class="col-12" id="tradingview" style="height: 550px;">
                                    </div>
                                </div>
                                <div class="col-6 row">
                                    <div class="col-6">
                                        <div class="col-12 text-center mb-2">
                                            <span class="badge badge-primary" id="refreshTrade">
                                                Trades
                                            </span>
                                        </div>
                                        <table class="table text-center">
                                            <thead class="thead-light">
                                                <tr>
                                                    <th scope="col">
                                                        Mua
                                                    </th>
                                                    <th scope="col">
                                                        Bán
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td>
                                                        <span class="count_buy_l text-success" data-symbol="">
                                                            0
                                                        </span>
                                                    </td>
                                                    <td>
                                                        <span class="count_sell_l text-danger" data-symbol="">
                                                            0
                                                        </span>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <span class="q_buy_l text-success" data-symbol="">
                                                            0
                                                        </span>
                                                    </td>
                                                    <td>
                                                        <span class="q_sell_l text-danger" data-symbol="">
                                                            0
                                                        </span>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td colspan="2">
                                                        <span class="rate_trades badge badge-success" data-symbol="">
                                                        </span>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="col-6">
                                        <div class="col-12 text-center mb-2">
                                            <span class="badge badge-primary" id="refreshOrder">
                                                Orders
                                            </span>
                                        </div>
                                        <table class="table text-center">
                                            <thead class="thead-light">
                                                <tr>
                                                    <th scope="col">
                                                        Mua
                                                    </th>
                                                    <th scope="col">
                                                        Bán
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td>
                                                        <span class="bids_q text-success" data-symbol="">
                                                            0
                                                        </span>
                                                    </td>
                                                    <td>
                                                        <span class="asks_q text-danger" data-symbol="">
                                                            0
                                                        </span>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td colspan="2">
                                                        <span class="rate_order badge badge-success" data-symbol="">
                                                        </span>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="col-12">
                                        <table class="table text-center" id="BuySellTable">
                                            <thead class="thead-light">
                                                <tr>
                                                    <th scope="col">
                                                        <div id="primaryCoin">
                                                            <span class="d-block name_coin" data-coin="">
                                                            </span>
                                                            <span class="value_coin" data-coin="">
                                                                0
                                                            </span>
                                                        </div>
                                                    </th>
                                                    <th scope="col">
                                                        <select class="form-control" id="percent">
                                                            <option value="5">
                                                                5%
                                                            </option>
                                                            <option value="10">
                                                                10%
                                                            </option>
                                                            <option value="25">
                                                                25%
                                                            </option>
                                                            <option value="50">
                                                                50%
                                                            </option>
                                                            <option value="75">
                                                                75%
                                                            </option>
                                                            <option selected="" value="100">
                                                                100%
                                                            </option>
                                                        </select>
                                                    </th>
                                                    <th scope="col">
                                                        <div id="altcoin">
                                                            <span class="d-block name_coin" data-coin="">
                                                                Alt
                                                            </span>
                                                            <span class="value_coin" data-coin="">
                                                                0
                                                            </span>
                                                        </div>
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td>
                                                        <input class="form-control" id="buy_price" placeholder="Price Buy" type="number"/>
                                                    </td>
                                                    <td>
                                                        <span class="price_buy" data-symbol="">
                                                            0.0000000
                                                        </span>
                                                    </td>
                                                    <td>
                                                        <input class="form-control" id="sell_price" placeholder="Price Sell" type="number"/>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <input class="form-control" id="buy_value" placeholder="BTC value" readonly="" type="number"/>
                                                    </td>
                                                    <td>
                                                        <span class="price_last" data-symbol="">
                                                            0.0000000
                                                        </span>
                                                    </td>
                                                    <td>
                                                        <input class="form-control bg bg-success text-white text-center" id="sell_profit" type="number" value="0"/>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <button class="btn btn-secondary" id="buy_coin">
                                                            Buy
                                                        </button>
                                                        <button class="btn btn-success" id="autobuy_coin">
                                                            Auto Buy
                                                        </button>
                                                    </td>
                                                    <td>
                                                        <span class="profit text-danger" data-symbol="">
                                                            (0.00%)
                                                        </span>
                                                    </td>
                                                    <td>
                                                        <button class="btn btn-secondary" id="sell_coin">
                                                            Sell
                                                        </button>
                                                        <button class="btn btn-danger" id="autosell_coin">
                                                            Auto Sell
                                                        </button>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div aria-hidden="true" aria-labelledby="smallModalLabel" class="modal fade" id="settingModal">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title">
                            Settings
                        </h5>
                        <button aria-label="Close" class="close" data-dismiss="modal" type="button">
                            <span aria-hidden="true">
                                ×
                            </span>
                        </button>
                    </div>
                    <!-- Modal body -->
                    <div class="modal-body">
                        <div class="container-fluid">
                            <form class="row" id="formSetting">
                                <input name="MarketName" type="hidden" value=""/>
                                <div class="col-6" id="settingMua">
                                    <div class="col-12">
                                        <span class="badge badge-success">
                                            Mua
                                        </span>
                                    </div>
                                    <div class="col-12 m-3">
                                        <input checked="" class="form-check-input" id="chienLuocMuaDay" name="chienLuocMua" type="radio" value="chienLuocMuaDay">
                                            <label class="form-check-label" for="chienLuocMuaDay">
                                                Mua BB & RSI
                                            </label>
                                        </input>
                                    </div>
                                    <div class="col-12 m-3">
                                        <div aria-controls="mocPriceBuy" aria-expanded="false" data-target="#mocPriceBuy" data-toggle="collapse">
                                            <input class="form-check-input" id="chienLuocMuaMoc" name="chienLuocMua" type="radio" value="chienLuocMuaMoc">
                                                <label class="form-check-label" for="chienLuocMuaMoc">
                                                    Mua Mốc
                                                </label>
                                            </input>
                                        </div>
                                        <div class="collapse" data-parent="#settingMua" id="mocPriceBuy">
                                            <input class="form-control w-75 d-inline-block is-valid" name="mocPriceBuy" type="number" value="0"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-6" id="settingBan">
                                    <div class="col-12">
                                        <span class="badge badge-danger">
                                            Bán
                                        </span>
                                    </div>
                                    <div class="col-12 m-3">
                                        <div aria-controls="collapseOne" aria-expanded="true" data-target="#collapseOne" data-toggle="collapse">
                                            <input checked="" class="form-check-input" id="chienLuocBanMin" name="chienLuocBan" type="radio" value="chienLuocBanMin">
                                                <label class="form-check-label" for="chienLuocBanMin">
                                                    Bán Gain
                                                </label>
                                                >=
                                                <input class="form-control d-inline-block" name="minGain" style="width:50px" value="">
                                                    %
                                                </input>
                                            </input>
                                        </div>
                                        <div aria-labelledby="headingOne" class="collapse show ml-3" data-parent="#settingBan" id="collapseOne">
                                            <div class="col-12">
                                                <input class="form-check-input" id="isCheckRsiBan" name="isCheckRsiBan" type="checkbox" value="1">
                                                    <label class="form-check-label" for="isCheckRsiBan">
                                                        Kiểm tra RSI > 80
                                                    </label>
                                                </input>
                                            </div>
                                            <div class="col-12">
                                                <input class="form-check-input" id="isCheckMACDBan" name="isCheckMACDBan" type="checkbox" value="1">
                                                    <label class="form-check-label" for="isCheckMACDBan">
                                                        Kiểm tra MACD
                                                    </label>
                                                </input>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-12 m-3">
                                        <div aria-controls="mocPriceSell" aria-expanded="false" data-target="#mocPriceSell" data-toggle="collapse">
                                            <input class="form-check-input" id="chienLuocBanMoc" name="chienLuocBan" type="radio" value="chienLuocBanMoc">
                                                <label class="form-check-label" for="chienLuocBanMoc">
                                                    Bán mốc
                                                </label>
                                            </input>
                                        </div>
                                        <div class="collapse" data-parent="#settingBan" id="mocPriceSell">
                                            <input class="form-control w-75 d-inline-block is-valid" name="mocPriceSell" type="number"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 text-center">
                                    <button class="btn btn-success" data-dismiss="modal" id="saveMarket">
                                        Save
                                    </button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div aria-hidden="true" aria-labelledby="smallModalLabel" class="modal fade" id="historyModel">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <!-- Modal body -->
                    <div class="modal-body">
                        <table class="table table-striped col-lg-12" id="history">
                            <thead>
                                <tr>
                                    <th>
                                        STT
                                    </th>
                                    <th>
                                        Thị trường
                                    </th>
                                    <th>
                                        Giá mua
                                    </th>
                                    <th>
                                        Giá bán
                                    </th>
                                    <th>
                                        Số lượng
                                    </th>
                                    <th>
                                        BTC mua
                                    </th>
                                    <th>
                                        BTC bán
                                    </th>
                                    <th>
                                        Tỉ lệ
                                    </th>
                                    <th>
                                        Thời gian bán
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <% for(var i in rows) { %>
                        <% var price_sell = rows[i].price_sell; %>
                        <% var price_buy = rows[i].price_buy; %>
                        <% var amount = rows[i].amount; %>
                        <% var market = rows[i].MarketName; %>
                        <% var timestamp = rows[i].timestamp.toISOString(); %>
                        <% var BTC_buy = price_buy * amount; %>
                        <% var BTC_sell = price_sell * amount; %>
                        <% var percent = rows[i].percent; %>
                                <tr>
                                    <td>
                                        <%= i %>
                                    </td>
                                    <td>
                                        <a class="fancybox" data-dismiss="modal" data-symbol="<%= market %>" data-target="#myModal" data-toggle="modal" href="#">
                                            <%= market %>
                                        </a>
                                    </td>
                                    <td class="text-success">
                                        <%= round(price_buy,8) %>
                                    </td>
                                    <td class="text-danger">
                                        <%= round(price_sell,8) %>
                                    </td>
                                    <td>
                                        <%= amount %>
                                    </td>
                                    <td class="text-success">
                                        <%= round(BTC_buy,8) %>
                                    </td>
                                    <td class="text-danger">
                                        <%= round(BTC_sell,8) %>
                                    </td>
                                    <td class="text-success">
                                        <%= percent %>%
                                    </td>
                                    <td>
                                        <%= timestamp %>
                                    </td>
                                </tr>
                                <% } %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <!-- plugins:js -->
        <script src="../dest/js/main.min.js">
        </script>
        <script src="../js/public.js">
        </script>
        <script src="../js/main.js">
        </script>
        <script src="../js/client.js">
        </script>
        <script src="https://s3.tradingview.com/tv.js" type="text/javascript">
        </script>
    </body>
</html>
